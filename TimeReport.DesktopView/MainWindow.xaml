<Window x:Class="TimeReport.DesktopView.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TimeReport.DesktopView"
        xmlns:properties="clr-namespace:TimeReport.DesktopView.Properties"
        mc:Ignorable="d"
        Title="{x:Static properties:Resources.MainWindowHeader}" Height="350" Width="525">
    <Grid>
        <Button x:Name="button" Content="{x:Static properties:Resources.SettingsButtonText}" HorizontalAlignment="Right" Margin="0,10,10,0" VerticalAlignment="Top"  Width="76" Height="28"/>
    </Grid>
</Window>
